<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Puppy Crawl//DTD Check Configuration 1.3//EN" "http://www.puppycrawl.com/dtds/configuration_1_3.dtd">
<module name="Checker">

    <module name="TreeWalker">
        <module name="MethodLength">
            <property name="max" value="10"/>
        </module>
        <module name="Indentation"></module>
        <module name="CyclomanicComplexity">
            <proterty name="max" value="3">
        </module>
        <module name="NestedIfDepth"></module>
        <module name="NestedForDepth"></module>
    </module>
    <module name="DuplicateCode">
        <module name="StrictDuplicateCode">
            <property name="min" value="5">
        </module>
    </module>

</module>
